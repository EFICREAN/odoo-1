	<menuitem id="base.menu_tablas" name="Tablas" parent="base.menu_base_partner" sequence="4"/>
    <menuitem id="urb_menu" name="Urbanizaciones" parent="base.menu_tablas" action="act_urb_id_act" sequence="1"/>
    <menuitem id="categind_menu" name="Categoría Industrial" parent="base.menu_tablas" action="categind_id_act" sequence="2"/>
    <menuitem id="codpostal_menu" name="Código Postal" parent="base.menu_tablas" action="codigopostal_act" sequence="3"/>
    <menuitem id="servcorp_menu" name="Servicios Corporativos" parent="base.menu_tablas" action="serv_corp_act" sequence="4"/>
  	<menuitem id="nomevento_menu" name="Nombre del Evento" parent="base.menu_tablas" action="nom_evento_act" sequence="5"/>
  </data>
</openerp>
 
