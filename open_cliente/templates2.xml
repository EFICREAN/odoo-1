    <record model='ir.ui.view' id='crm_leads_view'>
		<field name='name'>open_cliente.crm_leads_view</field>
		<field name='model'>crm.lead</field>
		<field name='inherit_id' ref='crm.crm_case_form_view_oppor'/>
		<field name='type'>form</field>
		<field name='arch' type='xml'>
			<field name="team_id" position="after">	
				<field name="x_serv_corp"/>
			</field>
			<notebook position="inside">
				<page string="Evento">
					<group>
					   <group>
							<field name="x_nro_personas_at"/>
							<field name="x_nro_horas_req"/>
							<field name="x_nro_dias_req"/>
							<field name="x_tipo_evento"/>
						</group>
						<group>
							<field name="x_razon_realiza_evento"/>
							<field name="x_nombre_evento"/>
							<field name="x_visita"/>
							<field name="x_nivel_interes"/>
					   </group>
				    </group>
				</page>
			</notebook>	
		</field>
	</record>

	<record model='ir.ui.view' id='product_category_view'>
		<field name='name'>open_cliente.product_category_view</field>
		<field name='model'>product.category</field>
		<field name='inherit_id' ref='product.product_category_form_view'/>
		<field name='type'>form</field>
		<field name='arch' type='xml'>
			<field name="parent_id" position="before">
					<field name="x_cod_prod"/>
			</field>	
		</field>
	</record>

  </data>
</openerp>

